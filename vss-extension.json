{
  "manifestVersion": 1,
  "id": "cky-flutter",
  "version": "0.0.5",
  "name": "Flutter",
  "description": "Flutter extension for Azure DevOps",
  "publisher": "kevinyang",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "icon.png"
  },
  "screenshots": [],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/chgc/vsts-flutter-tasks/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/chgc/vsts-flutter-tasks"
  },
  "tags": ["flutter", "mobile", "ios", "android"],
  "files": [
    {
      "path": "tasks/install"
    },
    {
      "path": "tasks/build"
    },
    {
      "path": "tasks/test"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "flutter-install",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/install"
      }
    },
    {
      "id": "flutter-build",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/build"
      }
    },
    {
      "id": "flutter-test",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/test"
      }
    }
  ]
}
